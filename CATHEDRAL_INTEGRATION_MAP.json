  "app_systems": {
    "design_suite": {
      "type": "python",
      "location": "design-suite/",
      "purpose": "Headless sacred geometry & fractal rendering",
      "exports_to": ["godot", "typescript", "json"],
      "modules": {
        "geometry": "Sacred geometry patterns (flower of life, merkaba, achad_tree, etc.)",
        "fractals": "L-system and recursive fractals",
        "render": "Matplotlib headless rendering",
        "palettes": "Color correspondence systems",
        "integrations": "achad_bridge, pathway mappings"
      }
    },
    "hall_of_mysteries": {
      "type": "python",
      "location": "hall-of-mysteries/",
      "purpose": "Frater Achad system & 22 pathway mechanics",
      "exports_to": ["design_suite", "godot", "json"],
      "modules": {
        "frater_achad_system": "Complete Achad tradition engine",
        "pathway_generator": "22 Hebrew letter True Will paths",
        "abyss_mechanics": "Oath of Abyss, Choronzon, Babalon gate"
      }
    },
    "liber_arcanae": {
      "type": "typescript",
      "location": "packages/liber-arcanae/",
      "purpose": "78-card tarot system with living archetypes",
      "imports_from": ["design_suite_json", "hall_of_mysteries_json"],
      "modules": {
        "types": "ArcanaCard, Character, GameDesign interfaces",
        "LiberArcanae": "Main tarot engine",
        "arcana": "Individual card implementations"
      }
    },
    "godot_liber_arcanae": {
      "type": "gdscript",
      "location": "godot/",
      "purpose": "Visual novel + RPG game implementation",
      "imports_from": ["design_suite_json", "liber_arcanae_json"],
      "scenes": {
        "studios": "Interactive labs for each tradition",
        "chapels": "Sacred spaces (Daath Crypt, etc.)",
        "characters": "Playable arcana beings"
      }
    },
    "stone_grimoire": {
      "type": "mixed",
      "location": "stone-grimoire/",
      "purpose": "Cathedral architecture & chapel system",
      "connects": ["all_characters", "all_research", "all_geometry"]
    },
    "cosmogenesis_learning_engine": {
      "type": "typescript",
      "location": "cosmogenesis-learning-engine/",
      "purpose": "Educational pathways through wisdom gates",
      "connects": [
        "144_codex_nodes",
        "99_dissolution_gates",
        "22_arcana_teachers"
      ]
    },
    "circuitum99": {
      "type": "typescript",
      "location": "packages/circuitum99/",
      "purpose": "RPG story system for character narratives",
      "connects": [
        "22_arcana_stories",
        "historical_relationships",
        "quest_systems"
      ]
    }
  },
  "cross_system_bridges": {
    "python_to_json": {
      "tool": "tools/export/combined_export.py",
      "generates": [
        "suite_export.json",
        "achad_export.json",
        "integration_spec.json"
      ]
    },
    "json_to_typescript": {
      "loaders": [
        "interop/web/achadLoader.ts",
        "packages/liber-arcanae/src/loaders/"
      ],
      "imports": "JSON specs from design-suite/outputs/"
    },
    "json_to_godot": {
      "loaders": [
        "interop/godot/achad_loader.gd",
        "godot/scripts/core/arcana_registry.gd"
      ],
      "imports": "JSON specs from design-suite/outputs/"
    },
    "research_gateway": {
      "purpose": "Serve public domain texts and research",
      "connects": ["all_historical_figures", "grimoires", "academic_sources"]
    }
  },
  "turborepo_config": {
    "packages": [
      "packages/liber-arcanae",
      "packages/circuitum99",
      "packages/game-engine",
      "packages/config",
      "packages/characters"
    ],
    "apps": ["cosmogenesis-learning-engine", "stone-grimoire"],
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", ".next/**"]
      },
      "test": {
        "dependsOn": ["build"]
      },
      "lint": {},
      "dev": {
        "cache": false
      }
    }
  },
