      "node": {
        "version": "20+",
        "package_manager": "pnpm 8+",
        "purpose": "Build orchestration, dev servers, package management"
      },
      "typescript": {
        "version": "5.6.3",
        "config": "tsconfig.json per package",
        "purpose": "Type-safe development across all packages and apps"
      },
      "vite": {
        "version": "5.4.21",
        "role": "Module bundler, static site generator",
        "output": "apps/*/dist/ â†’ GitHub Pages"
      },
      "turbo": {
        "version": "2.1.3",
        "role": "Monorepo orchestration, incremental builds",
        "config": "turbo.json"
      }
    },
    "runtime_browser": {
      "react": {
        "version": "18.3.1",
        "role": "UI framework for all web apps",
        "apps": ["liber-arcanae-tarot", "web", "synth-lab", "tarot-arena"]
      },
      "three_js": {
        "packages": ["@react-three/fiber@8.16.8", "@react-three/drei@9.114.0"],
        "role": "WebGL 3D rendering for sacred geometry",
        "apps": ["web", "rosslyn-explorer"]
      },
      "web_audio": {
        "role": "Audio synthesis for mystical soundscapes",
        "apps": ["synth-lab"]
      }
    },
    "optional_platforms": {
      "godot": {
        "version": "4.x",
        "packages": [
          "godot-design-studio",
          "godot-liber-arcanae",
          "godot-vfx-library",
          "godot-codex-14499"
        ],
        "integration": "Reads JSON from Python design-suite",
        "status": "Prepared, not required for web deployment"
      },
      "bevy": {
        "status": "Future research (Rust game engine)"
      }
    },
    "governance": {
      "openspec": {
        "location": "openspec/",
        "files": ["cathedral.spec.json", "schema.json", "AGENTS.md"],
        "role": "Living documentation, spec-driven development, AI collaboration"
      }
    },
    "deployment_model": "Static site generation (no server runtime, no cloud dependencies)",
    "independence": "Runs entirely in browser after build - zero Azure/Python/Node runtime deps"
