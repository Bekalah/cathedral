# Azure Configuration for Cathedral of Circuits
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key-here
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini

# Azure Storage for Permanent Datasets
AZURE_STORAGE_ACCOUNT=your-storage-account-name
AZURE_STORAGE_KEY=your-storage-account-key
AZURE_CONTAINER_NAME=cathedral-datasets

# Azure ML GPU for Art/Sound Generation
AZURE_ML_WORKSPACE=your-ml-workspace-name
AZURE_ML_COMPUTE=gpu-compute-cluster
AZURE_ML_EXPERIMENT=cathedral-art-generation

# Application Configuration
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:5173

# Cathedral of Circuits Configuration
CATHEDRAL_MODE=development
TRAUMA_SAFETY=maximum
FUSION_KINK_ENABLED=true
LIVING_ARCANAE_ACTIVE=true
